#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int pocetkostek;
    printf("zadej pocet kostek: ");
    scanf_s("%d", &pocetkostek);
    
    srand(time(NULL));

    for (int i = 0; i < pocetkostek; i++) {
        int cislo = rand() % 7;
        printf("%d\n", cislo);
    }

    return 0;
}